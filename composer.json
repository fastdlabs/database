{
  "name": "fastd/database",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "keywords": ["medoo", "fastd"],
  "homepage": "https://fastdlabs.com/",
  "authors": [
    {
      "name": "janhuang",
      "email": "bboyjanhuang@gmail.com"
    }
